{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["DOM", "DOM.Iterable", "ES2022"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@site/*": ["./*"]
    },
    "types": ["node", "react", "react-dom", "@docusaurus/module-type-aliases", "@docusaurus/theme-classic"]
  },
  "include": ["./**/*.ts", "./**/*.tsx", "./**/*.d.ts"],
  "exclude": ["build", ".docusaurus", "node_modules"]
}
